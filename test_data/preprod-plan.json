{
  "format_version": "1.2",
  "terraform_version": "1.5.0",
  "resource_changes": [
    {
      "address": "aws_instance.web",
      "type": "aws_instance",
      "change": {
        "actions": ["update"],
        "before": {
          "ami": "ami-preprod123",
          "instance_type": "t2.large",
          "tags": {
            "Environment": "PreProd",
            "Name": "web-server-preprod"
          }
        },
        "after": {
          "ami": "ami-preprod123",
          "instance_type": "t2.large",
          "tags": {
            "Environment": "PreProd",
            "Name": "web-server-preprod"
          }
        }
      }
    },
    {
      "address": "aws_s3_bucket.data",
      "type": "aws_s3_bucket",
      "change": {
        "actions": ["update"],
        "before": {
          "bucket": "preprod-data-bucket",
          "versioning": true,
          "tags": {
            "Environment": "PreProd"
          }
        },
        "after": {
          "bucket": "preprod-data-bucket",
          "versioning": true,
          "tags": {
            "Environment": "PreProd"
          }
        }
      }
    }
  ]
}
